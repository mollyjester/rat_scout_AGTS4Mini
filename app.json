{
  "configVersion": "v2",
  "app": {
    "appId": 1000089,
    "appName": "Rat Scout",
    "appType": "watchface",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "mollyjester",
    "description": "Rat Scout watchface for Amazfit GTS 4 Mini"
  },
  "permissions": [
    "device:os.date",
    "data:user.hd.step_count",
    "data:user.hd.heart_rate",
    "device:basic.battery"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "1.0.0",
      "target": "1.0.1",
      "minVersion": "1.0.0"
    }
  },
  "targets": {
    "gts4mini": {
      "module": {
        "watchface": {
          "path": "watchface/index"
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "name": "gts4mini",
          "deviceSource": 229
        }
      ],
      "designWidth": 336
    }
  },
  "i18n": {
    "en-US": {
      "appName": "Rat Scout"
    }
  },
  "defaultLanguage": "en-US"
}
